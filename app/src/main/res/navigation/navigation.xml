<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homePage">
    <fragment
        android:id="@+id/homePage"
        android:name="com.raminarmanfar.learngermanapp.HomePage"
        android:label="fragment_home_page"
        tools:layout="@layout/fragment_home_page">
        <action
            android:id="@+id/action_homePage_to_herrProfessorList"
            app:destination="@+id/herrProfessorList"
            app:enterAnim="@anim/fragmet_enter"
            app:exitAnim="@anim/fragment_exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
    </fragment>
    <fragment
        android:id="@+id/herrProfessorList"
        android:name="com.raminarmanfar.learngermanapp.HerrProfessorList"
        android:label="fragment_herr_professor_list"
        tools:layout="@layout/fragment_herr_professor_list" />
</navigation>